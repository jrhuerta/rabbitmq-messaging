    "version": "0.2.0",
    "configurations": [
        {
            "name": "consumer",
            "type": "python",
            "request": "launch",
            "module": "alaya.events.consumer",
            "args": [
                "-e",
                "component_name-events"
            ]
        },
        {
            "name": "outbox",
            "type": "python",
            "request": "launch",
            "module": "alaya.events.outbox",
            "args": [
                "-e",
                "component_name-events"
            ]
        },
        {
            "name": "app",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "alaya.demo.app:app",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "invoke",
            "type": "python",
            "request": "launch",
            "module": "invoke",
            "args": [
                "db",
                "--count",
                "1"
            ],
        }
    ]
}